{
    "name": "sagradafamilia",
    "version": "1.0.0",
    "description": "aplicación para consultorio médico.",
    "main": "main.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "node src/index.js"

    },
    "build": {
        "appId": "Consultorios",
        "asar": true,
        "dmg": {
            "contents": [{
                    "x": 110,
                    "y": 150
                },
                {
                    "x": 240,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                }
            ]
        },
        "linux": {
            "target": [
                "AppImage",
                "deb"
            ]
        },
        "win": {
            "target": "NSIS",
            "icon": "build/icono.ico"
        }
    },
    "keywords": [],
    "author": "utn",
    "license": "MIT",
    "dependencies": {
        "@trodi/electron-splashscreen": "^0.3.4",
        "body-parser": "^1.18.3",
        "connect-flash": "^0.1.1",
        "cookie-parser": "^1.4.3",
        "ejs": "^2.6.1",
        "electron-packager": "^14.0.1",
        "electron-prebuilt": "^1.4.13",
        "express": "^4.16.3",
        "express-session": "^1.15.6",
        "jquery": "^3.4.1",
        "js-yaml": "^3.13.1",
        "morgan": "^1.9.1",
        "mysql": "^2.16.0",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "serve-favicon": "^2.5.0"
    },
    "devDependencies": {
        "electron": "^3.0.0",
        "electron-builder": "^20.36.2",
        "nodemon": "^1.18.4"
    }
